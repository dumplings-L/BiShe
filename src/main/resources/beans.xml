</bean>
    <bean id="myNissan" class="personal.xjl.jerrymouse.spring.Car">
        <constructor-arg name="brand" value="Missan"/>
        <constructor-arg name="type" value="xunayi"/>
        <constructor-arg name="maxSpeed" value="188"/>
    </bean>

</beans>